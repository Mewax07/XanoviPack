{
  "name": "xanovi_lib",
  "description": "XanoviPack est une librairie Node.js qui centralise les librairies de Literate. Elle ajoute également d'autres fonctionnalités peu utilisées mais présentes sur les différentes plateformes",
  "authors": "Mewax07",
  "version": "1.0.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    "import": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    },
    "require": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "bun build src/index.ts --outdir dist --target bun --format esm,cjs --minify",
    "parse": "bun run scripts/generator.ts",
    "readme": "bun run scripts/readme.ts"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@types/node": "^24.3.0",
    "@types/node-forge": "^1.3.14",
    "@types/pako": "^2.0.4",
    "@types/set-cookie-parser": "^2.4.10",
    "bun": "^1.2.21",
    "node-forge": "^1.3.1",
    "pako": "^2.1.0",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.2.0",
    "set-cookie-parser": "^2.7.1"
  },
  "type": "module",
  "private": true,
  "devDependencies": {
    "@types/bun": "^1.2.21"
  },
  "peerDependencies": {
    "typescript": "^5.9.2"
  }
}
