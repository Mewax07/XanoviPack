{
	"name": "xanovi_lib",
	"description": "XanoviPack est une librairie Node.js qui centralise les librairies de Literate. Elle ajoute également d'autres fonctionnalités peu utilisées mais présentes sur les différentes plateformes",
	"authors": "Mewax07",
	"version": "1.0.0",
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"exports": {
		"import": {
			"types": "./dist/index.d.mts",
			"default": "./dist/index.mjs"
		},
		"require": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.js"
		}
	},
	"scripts": {
		"build": "bun build src/index.ts --outdir dist --target bun --format esm,cjs --minify",
		"parse": "bun run scripts/generator.ts",
		"readme": "bun run scripts/readme.ts"
	},
	"files": [
		"dist"
	],
	"dependencies": {
		"@noble/ciphers": "^2.0.1",
		"@noble/hashes": "^2.0.1",
		"@scure/base": "^2.0.0",
		"@tauri-apps/plugin-http": "^2.5.2",
		"cheerio": "^1.1.2",
		"fast-xml-parser": "^5.2.5",
		"fflate": "^0.8.2",
		"micro-rsa-dsa-dh": "^0.2.3",
		"undici": "^7.15.0"
	},
	"type": "module",
	"private": true,
	"devDependencies": {
		"prettier": "^3.6.2",
		"prettier-plugin-organize-imports": "^4.2.0",
		"@types/node": "^24.3.0",
		"@types/bun": "^1.2.21"
	},
	"peerDependencies": {
		"typescript": "^5.9.2"
	}
}
