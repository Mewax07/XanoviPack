{
	"compilerOptions": {
		"target": "esnext",
		"module": "esnext",
		"moduleResolution": "bundler",
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"useDefineForClassFields": true,
		"paths": {
			"~p0/*": ["./pawnote_r0/src/*"],
			"~p0_rw/*": ["./pawnote_r0_rewrite/src/*"],
			"~d0/*": ["./utils/desero/src/*"],
			"~s0/*": ["./utils/schwi/src/*"],
			"~_maq/*": ["./utils/maq/src/*"],
			"~f0/*": ["./fetcher/*"],
			"~m0/*": ["./mbown/*"]
		},
		"strict": true,
		"noEmit": true,
		"skipLibCheck": true,
		"isolatedModules": true,
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true
	},
	"include": [
		"pawnote_r0/src/**/*",
		"./pawnote_r0_rewrite/src/**/*",
		"src/**/*",
		"utils/desero/src/**/*",
		"utils/schwi/src/**/*",
		"fetcher/*",
		"mbown/**/*"
	],
	"exclude": ["node_modules", "dist", "ignored"]
}
