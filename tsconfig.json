{
	"compilerOptions": {
		"target": "esnext",
		"module": "esnext",
		"moduleResolution": "bundler",
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"paths": {
			"~p0/*": ["./pawnote_r0/src/*"],
			"~d0/*": ["./utils/desero/src/*"],
			"~s0/*": ["./utils/schwi/src/*"],
			"~_maq/*": ["./utils/maq/src/*"],
		},
		"strict": true,
		"noEmit": true,
		"skipLibCheck": true,
		"isolatedModules": true,
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true
	},
	"include": ["pawnote_r0/src/**/*", "src/**/*", "utils/desero/src/**/*", "utils/schwi/src/**/*"],
	"exclude": ["node_modules", "dist", "ignored"]
}
